@model IEnumerable<DoctorManagement.ViewModel.StatisticDateGroup>

@{ 
    ViewBag.Title = "Patient Body Statistics";

}
<h2>Patient Body Statistics</h2>


    <table>
        <tr>
            <th>
                Enrollment Date
            </th>
            <th>
                Patients
            </th>
        </tr>
        @foreach (var item in Model)
        {
            <tr>
                <td>@Html.DisplayFor(modelItem => item.Date)</td>
                <td> @item.PatientCount</td>
            </tr>
        }
    </table>
